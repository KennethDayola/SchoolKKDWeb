.tables
SELECT * FROM users;
;
PRAGMA table_info(users);
DELETE FROM users WHERE username = 'sean123';
.save
.exit
.tables
SELECT * FROM users;
SELECT * FROM users WHERE column_name IS NULL;
SELECT * FROM users WHERE users  IS NULL;
DELETE FROM users WHERE username = null
;
SELECT * FROM users;
DELETE FROM users WHERE username IS NULL;
.save
.exit
